{
  "name": "protobuf-json-loader",
  "version": "1.0.2",
  "description": "A protobuf loader module for webpack, transfer proto to json",
  "license": "MIT",
  "author": "Alan Chan",
  "keywords": [
    "proto",
    "protobuf",
    "protobufjs",
    "protobuf.js",
    "loader"
  ],
  "bin": "",
  "main": "src/index.js",
  "engines": {
    "node": ">= 6.9.0 < 7.0.0 || >= 8.9.0"
  },
  "scripts": {
