    "html-webpack-plugin": "^3.2.0",
    "protobuf-loader": "^0.0.8",
    "webpack": "^4.27.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
